using output;

fun stringEqTest(str1: like "foo", str2: like "bar") is
  var condition: like true;
  condition = str1 == str2;
  if condition then
    put("strings equal");
    putln;
  else
    put("strings not equal");
    putln;
  end;
end;

stringEqTest("equal", "equal");
stringEqTest("equal", "notequal");
